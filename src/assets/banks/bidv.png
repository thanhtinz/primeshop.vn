   /* Button */
.nwp-btn {
  min-width: 136px;
  height: 56px;
  line-height: 56px;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  border-radius: var(--corner-radius);
  border: 1.5px solid transparent;
}

.nwp-btn-sizeM {
  height: 48px;
  line-height: 48px;
}

.nwp-btn span {
  font-family: var(--nwp-body-font);
  font-size: 16px;
  font-weight: 500;
  line-height: 24px;
  letter-spacing: 0.32px;
}

.nwp-btnPrimary {
  background-color: var(--primary-500);
  color: var(--neutral-white);
}

.nwp-btnPrimaryBG {
  background: var(--gold-button);
  color: var(--primary-500);
}

.nwp-btnSecondary {
  background-color: transparent;
  border: 1.5px solid var(--primary-500);
  color: var(--primary-500);
}

.nwp-btnSecondaryBG {
  border: 1.5px solid var(--neutral-white);
  color: var(--neutral-white);
}

.nwp-btnDisable {
  color: var(--neutral-white);
  background-color: var(--neutral-disable);
  pointer-events: none;
}

.nwp-btnPrimary:hover,
.nwp-btnPrimaryBG:hover,
.nwp-btnSecondary:hover,
.nwp-btnSecondaryBG:hover {
  border: 1.5px solid transparent;
  background:
    linear-gradient(var(--primary-600), var(--primary-600)) padding-box,
    var(--gold-line) border-box;
}

.nwp-btnPrimary:hover span,
.nwp-btnPrimaryBG:hover span,
.nwp-btnSecondary:hover span,
.nwp-btnSecondaryBG:hover span {
  background: var(--gold-text-02);
  background-clip: text;
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}

