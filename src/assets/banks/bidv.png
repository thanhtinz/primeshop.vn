  --corner-radius: 4px;
  --BIDV-gold: #d4af62;
  --gold-text-01: linear-gradient(90deg, #d4af62 0.4%, #f6dfa0 50.12%, #dac07f 99.83%),
    linear-gradient(90deg, #ce9744 0%, #ffd686 54.69%, #ce9845 100%);
  --gold-text-02: linear-gradient(53deg, #cd9542 0.16%, #fce8a0 58.43%, #cd9542 100.05%);
  --gold-text-03: linear-gradient(90deg, #ce9744 0%, #ffd686 54.69%, #ce9845 100%);
