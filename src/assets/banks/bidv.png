  --neutral-text-primary: #292929;
  --neutral-text-secondary: #575757;
  --neutral-disable: #ccc;
  --neutral-divider: #e3e3e3;
  --neutral-5: #ededed;
  --neutral-background: #fafafa;
  --neutral-white: #fff;
